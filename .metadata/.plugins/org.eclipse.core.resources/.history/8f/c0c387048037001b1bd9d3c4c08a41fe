package exceptionHandling;

public class UncaughtException {

	public static void main(String[] args) {
		System.out.println("A");
		
		try {
			System.out.println(40*2/0);
			throw new Exception();
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.getMessage();
		}
	
		System.out.println("B");

	}

}
